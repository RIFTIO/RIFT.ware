<cli>
  <plugin-list> 
     <plugin name="rwcli_plugin" typelib="rwcli_plugin/1.0"/>
  </plugin-list>

  <defaults>
    <defaultPrintHook hook="rwcli_plugin:default_print"/>
    <configPrintHook hook="rwcli_plugin:config_writer"/>
    <configPrintToFileHook hook="rwcli_plugin:config_writer_file"/>
    <prettyPrintXmlHook hook=".:pretty_print_xml"/>
    <prettyPrintXmlToFileHook hook=".:pretty_print_xml_file"/>
  </defaults>
  
</cli>
