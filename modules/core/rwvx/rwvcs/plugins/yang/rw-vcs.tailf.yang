module rw-vcs-annotation
{
  namespace "http://riftio.com/ns/riftware-1.0/rw-vcs";
  prefix "rw-vcs-ann";

  import rw-vcs {
    prefix vcs;
  }

  import tailf-common {
    prefix tailf;
  }

  tailf:annotate "/vcs:vstart" {
    tailf:actionpoint rw_actionpoint;
  }

  tailf:annotate "/vcs:vstop" {
    tailf:actionpoint rw_actionpoint;
  }

  tailf:annotate "/vcs:vcrash" {
    tailf:actionpoint rw_actionpoint;
  }

  tailf:annotate "/vcs:vsnap" {
    tailf:actionpoint rw_actionpoint;
  }
}
